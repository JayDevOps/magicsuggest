{
  "name": "magicsuggest",
  "description": "MagicSuggest is a multiple selection auto suggest combo box for Bootstrap 3.",
  "version": "3.0.0",
  "keywords": [
    "bootstrap",
    "css",
    "jquery",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "http://nicolasbize.com/magicsuggest",
  "author": "The magicsuggest Authors (https://github.com/magicsuggest/magicsuggest/graphs/contributors)",
  "contributors": [
    "Nicolasblaze"
  ],
  "scripts": {
    "bundlesize": "bundlesize",
    "css": "npm-run-all --parallel css-lint* css-compile* --sequential css-prefix* css-minify*",
    "css-main": "npm-run-all --parallel css-lint css-compile --sequential css-prefix css-minify",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/magicsuggest.scss dist/magicsuggest.css",
    "css-lint": "stylelint --syntax scss \"scss/**/*.scss\"",
    "css-lint-vars": "node build/lint-vars.js scss/ assets/scss/",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/*.css\" \"!dist/*.min.css\"",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/magicsuggest.min.css dist/magicsuggest.css",
    "js": "npm-run-all js-lint* js-compile js-minify",
    "js-main": "npm-run-all js-lint js-compile js-minify",
    "js-lint": "eslint js/ build/",
    "js-compile": "npm-run-all --parallel js-compile-*",
    "js-compile-standalone": "rollup --environment BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-compile-bundle": "rollup --environment BUNDLE:true --config build/rollup.config.js --sourcemap",
    "js-compile-plugins": "cross-env PLUGINS=true babel js/src/ --out-dir js/dist/ --source-maps",
    "js-minify": "npm-run-all --parallel js-minify-*",
    "js-minify-standalone": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/magicsuggest.js.map,includeSources,url=magicsuggest.min.js.map\" --output dist/magicsuggest.min.js dist/magicsuggest.js",
    "js-minify-bundle": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/magicsuggest.bundle.js.map,includeSources,url=magicsuggest.bundle.min.js.map\" --output dist/magicsuggest.bundle.min.js dist/magicsuggest.bundle.js",
    "js-test": "npm-run-all --parallel js-test-karma*",
    "js-test-karma": "karma start js/tests/karma.conf.js",
    "js-test-karma-old": "cross-env USE_OLD_JQUERY=true npm run js-test-karma",
    "js-test-cloud": "ruby -r webrick -e \"s = WEBrick::HTTPServer.new(:Port => 3000, :DocumentRoot => Dir.pwd, :Logger => WEBrick::Log.new('/dev/null'), :AccessLog => []); trap('INT') { s.shutdown }; s.start\" & node build/saucelabs-unit-test.js",
    "maintenance-dependencies": "ncu -a -x jquery && npm update && bundle update && shx echo \"Manually update assets/js/vendor/*, js/tests/vendor/* and .travis.yml\"",
    "release-sri": "node build/generate-sri.js",
    "release-version": "node build/change-version.js",
    "release-zip": "cd dist/ && zip -r9 magicsuggest-$npm_package_version-dist.zip * && shx mv magicsuggest-$npm_package_version-dist.zip ..",
    "dist": "npm-run-all --parallel css js",
    "test": "npm-run-all dist js-test docs-compile docs-lint bundlesize",
    "watch": "npm-run-all --parallel watch-css watch-js",
    "watch-css": "nodemon --ignore js/ --ignore dist/ -e scss -x \"npm run css\"",
    "watch-js": "nodemon --ignore scss/ --ignore js/dist/ --ignore dist/ -e js -x \"npm run js-compile\""
  },
  "style": "dist/magicsuggest.css",
  "sass": "scss/magicsuggest.scss",
  "main": "dist/magicsuggest",
  "repository": "git+https://github.com/magicsuggest/magicsuggest.git",
  "bugs": {
    "url": "https://github.com/magicsuggest/magicsuggest/issues"
  },
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "1.9.1 - 3"
  },
  "peerDependencies": {
    "jquery": "1.9.1 - 3"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.38",
    "@babel/core": "7.0.0-beta.38",
    "@babel/preset-env": "7.0.0-beta.38",
    "autoprefixer": "^7.2.5",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-es2015-modules-strip": "^0.1.1",
    "bundlesize": "^0.15.3",
    "clean-css-cli": "^4.1.10",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-plugin-compat": "^2.1.0",
    "glob": "^7.1.2",
    "htmllint-cli": "^0.0.6",
    "jsunitsaucelabs": "^1.3.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-detect-browsers": "^2.2.6",
    "karma-firefox-launcher": "^1.1.0",
    "karma-qunit": "^1.2.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.12.9",
    "postcss-cli": "^4.1.1",
    "qunitjs": "^2.4.1",
    "rollup": "^0.54.1",
    "rollup-plugin-babel": "4.0.0-beta.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "shelljs": "^0.8.1",
    "shx": "^0.2.2",
    "sri-toolbox": "^0.2.0",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0",
    "uglify-js": "^3.3.8",
    "vnu-jar": "^17.11.1",
    "workbox-build": "^2.1.2"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist/",
    "js/{src,dist}/",
    "scss/"
  ],
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "bundlesize": [
    {
      "path": "./dist/magicsuggest.css",
      "maxSize": "20 kB"
    },
    {
      "path": "./dist/magicsuggest.min.css",
      "maxSize": "10 kB"
    },
    {
      "path": "./dist/magicsuggest.js",
      "maxSize": "75 kB"
    },
    {
      "path": "./dist/magicsuggest.min.js",
      "maxSize": "25 kB"
    }
  ],
  "jspm": {
    "registry": "npm",
    "main": "js/magicsuggest",
    "directories": {
      "lib": "dist"
    },
    "shim": {
      "js/bootstrap": {
        "deps": [
          "jquery",
          "bootstrap"
        ],
        "exports": "$"
      }
    },
    "dependencies": {},
    "peerDependencies": {
      "jquery": "1.9.1 - 3",
      "bootstrap": "^3.3.7"
    }
  }
}
