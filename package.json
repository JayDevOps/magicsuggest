{
  "name": "magicsuggest",
  "description": "MagicSuggest is a multiple selection auto suggest combo box for Bootstrap 3.",
  "version": "3.0.0",
  "keywords": [
    "bootstrap",
    "css",
    "jquery",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "http://nicolasbize.com/magicsuggest",
  "author": "The magicsuggest Authors (https://github.com/magicsuggest/magicsuggest/graphs/contributors)",
  "contributors": [
    "Nicolasblaze"
  ],
  "scripts": {
    "css": "npm-run-all --parallel css-compile* --sequential css-prefix* css-minify*",
    "css-main": "npm-run-all --parallel css-compile --sequential css-prefix css-minify",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/magicsuggest.scss dist/magicsuggest.css",
    "css-prefix": "postcss --replace \"dist/*.css\" \"!dist/*.min.css\"",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/magicsuggest.min.css dist/magicsuggest.css",
    "js": "npm-run-all --parallel js-cp js-minify",
    "js-cp": "node copysrc.js",
    "js-main": "npm-run-all --parallel js-cp js-minify",
    "js-minify": "npm-run-all --parallel js-minify-*",
    "js-minify-standalone": "uglifyjs js/src/magicsuggest.js --compress typeofs=false --mangle --comments \"/^!/\" --output dist/magicsuggest.min.js",
    "dist": "npm-run-all --parallel css js"
  },
  "style": "dist/magicsuggest.css",
  "sass": "scss/magicsuggest.scss",
  "main": "dist/magicsuggest",
  "repository": "git+https://github.com/magicsuggest/magicsuggest.git",
  "bugs": {
    "url": "https://github.com/magicsuggest/magicsuggest/issues"
  },
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "1.9.1 - 3"
  },
  "peerDependencies": {
    "jquery": "1.9.1 - 3"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.38",
    "@babel/core": "7.0.0-beta.38",
    "@babel/preset-env": "7.0.0-beta.38",
    "autoprefixer": "^7.2.5",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-es2015-modules-strip": "^0.1.1",
    "bundlesize": "^0.15.3",
    "clean-css-cli": "^4.1.10",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-plugin-compat": "^2.1.0",
    "glob": "^7.1.2",
    "htmllint-cli": "^0.0.6",
    "jsunitsaucelabs": "^1.3.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-detect-browsers": "^2.2.6",
    "karma-firefox-launcher": "^1.1.0",
    "karma-qunit": "^1.2.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.12.9",
    "postcss-cli": "^4.1.1",
    "qunitjs": "^2.4.1",
    "rollup": "^0.54.1",
    "rollup-plugin-babel": "4.0.0-beta.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "shelljs": "^0.8.1",
    "shx": "^0.2.2",
    "sri-toolbox": "^0.2.0",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0",
    "uglify-js": "^3.3.8",
    "vnu-jar": "^17.11.1",
    "workbox-build": "^2.1.2"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist/",
    "js/src/",
    "scss/"
  ],
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "jspm": {
    "registry": "npm",
    "main": "js/magicsuggest",
    "directories": {
      "lib": "dist"
    },
    "shim": {
      "js/bootstrap": {
        "deps": [
          "jquery",
          "bootstrap"
        ],
        "exports": "$"
      }
    },
    "dependencies": {},
    "peerDependencies": {
      "jquery": "1.9.1 - 3",
      "bootstrap": "^3.3.7"
    }
  }
}
